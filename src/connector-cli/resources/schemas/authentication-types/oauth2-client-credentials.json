{
  "$schema": "https://json-schema.org/draft-07/schema",
  "title": "OAuth2ClientCredentials",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "clientId": {
      "type": "string"
    },
    "clientSecret": {
      "type": "string"
    },
    "scope": {
      "type": "string"
    },
    "tokenEndpoint": {
      "type": "string"
    }
  },
  "required": ["clientId", "clientSecret", "tokenEndpoint"],
  "additionalProperties": false
}
